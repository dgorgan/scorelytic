{
  "name": "scorelytic",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "check:types": "tsc --noEmit -p server/tsconfig.json && tsc --noEmit -p client/tsconfig.json",
    "test:server": "cd server && npx jest",
    "test:client": "cd client && npx jest",
    "check:all": "npm run check:types && npm run test:server && npm run test:client",
    "dev": "concurrently \"npm run dev --prefix server\" \"npm run dev --prefix client\"",
    "dev:server": "ts-node-dev --respawn --transpile-only server/src/server.ts",
    "migrate": "dotenv -e .env -- node scripts/check-and-migrate.js",
    "seed:server": "dotenv -e server/.env -- ts-node server/src/utils/seedSupabase.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.3",
    "concurrently": "^9.1.0",
    "dotenv-cli": "^8.0.0",
    "jest": "^29.7.0",
    "pg": "^8.16.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  },
  "dependencies": {
    "@supabase/gotrue-js": "^2.70.0",
    "@supabase/postgrest-js": "^1.19.4",
    "@supabase/supabase-js": "^2.49.8",
    "camelcase-keys": "^9.1.3",
    "openai": "^4.103.0",
    "snakecase-keys": "^8.0.1"
  }
}
