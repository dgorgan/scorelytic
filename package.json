{
  "name": "scorelytic",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "clean": "rm -rf .turbo node_modules **/dist",
    "clean:deep": "rm -rf .turbo node_modules **/dist **/tsconfig.tsbuildinfo",
    "rebuild": "pnpm exec tsc -b --clean && pnpm exec tsc -b",
    "build:ts": "pnpm exec tsc -b",
    "check:types": "tsc --noEmit -p server/tsconfig.json && tsc --noEmit -p client/tsconfig.json",
    "test:server": "cd server && jest --coverage --coverageDirectory=../coverage/server",
    "test:client": "cd client && jest --coverage --coverageDirectory=../coverage/client",
    "test:shared": "cd shared && jest --coverage --coverageDirectory=../coverage/shared",
    "test": "jest --coverage",
    "test:fast": "jest",
    "test:changed": "jest --onlyChanged",
    "coverage:merge": "nyc merge coverage coverage/coverage-final.json && nyc report --reporter=lcov --reporter=text",
    "coverage": "pnpm run test && pnpm run coverage:merge",
    "check:all": "pnpm run check:types && pnpm run test",
    "dev": "turbo run dev",
    "dev:server": "ts-node-dev --respawn --transpile-only server/src/server.ts",
    "migrate": "dotenv -e .env -- node scripts/check-and-migrate.js",
    "seed:server": "dotenv -e server/.env -- ts-node server/src/utils/seedSupabase.ts",
    "batch:llm-test": "dotenv -e server/.env -- npx ts-node -P scripts/tsconfig.json scripts/batch-llm-review-test.ts",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "turbo run format",
    "type-check": "turbo run type-check",
    "build": "turbo run build"
  },
  "workspaces": [
    "client",
    "server",
    "shared",
    "scripts"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@emotion/jest": "^11.13.0",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.18",
    "@types/d3-array": "^3.2.1",
    "@types/d3-color": "^3.1.3",
    "@types/d3-ease": "^3.0.2",
    "@types/d3-interpolate": "^3.0.4",
    "@types/d3-path": "^3.1.1",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-shape": "^3.1.7",
    "@types/d3-time": "^3.0.4",
    "@types/d3-timer": "^3.0.2",
    "@types/dotenv": "^8.2.3",
    "@types/eslint": "^9.6.1",
    "@types/eslint-scope": "^8.3.0",
    "@types/estree": "^1.0.7",
    "@types/express": "^4.17.21",
    "@types/geojson": "^7946.0.16",
    "@types/geojson-vt": "^3.2.5",
    "@types/istanbul-lib-coverage": "^2.0.6",
    "@types/istanbul-lib-report": "^3.0.3",
    "@types/istanbul-reports": "^3.0.4",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/json-schema": "^7.0.15",
    "@types/json5": "^0.0.30",
    "@types/less": "^3.0.8",
    "@types/mapbox__point-geometry": "^0.1.4",
    "@types/mapbox__vector-tile": "^1.3.4",
    "@types/node": "^22.15.28",
    "@types/papaparse": "^5.3.16",
    "@types/pbf": "^3.0.5",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/serve-static": "^1.15.7",
    "@types/stylus": "^0.48.43",
    "@types/supertest": "^6.0.3",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/testing-library__react": "^10.0.1",
    "@types/tough-cookie": "^4.0.5",
    "@types/ws": "^8.18.1",
    "@types/yargs": "^17.0.33",
    "@types/youtube-captions-scraper": "^1.0.0",
    "concurrently": "^9.1.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.3.2",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "lint-staged": "^15.5.2",
    "next": "^15.3.2",
    "nyc": "^17.1.0",
    "pg": "^8.16.0",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "turbo": "^1.13.0",
    "typescript": "^5.8.3",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "@logtail/node": "^0.5.5",
    "@logtail/pino": "^0.5.5",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/gotrue-js": "^2.70.0",
    "@supabase/postgrest-js": "^1.19.4",
    "@supabase/supabase-js": "^2.49.8",
    "body-parser": "^1.20.2",
    "camelcase-keys": "^9.1.3",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.5.0",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "module-alias": "^2.2.3",
    "openai": "^4.103.0",
    "papaparse": "^5.5.3",
    "react-plotly.js": "^2.6.0",
    "snakecase-keys": "^8.0.1",
    "youtube-captions-scraper": "^2.0.3"
  },
  "type": "module",
  "lint-staged": {
    "*.{js,ts,tsx,jsx,json,md,yml,yaml,css,scss,html}": [
      "prettier --write"
    ]
  },
  "nyc": {
    "all": true,
    "extension": [
      ".js",
      ".ts",
      ".tsx"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": [
      "**/node_modules/**",
      "**/test/**",
      "**/*.d.ts"
    ],
    "temp-directory": "./coverage/.nyc_output"
  },
  "packageManager": "pnpm@10.11.0"
}
